class Solution {
    public int repeatedNTimes(int[] nums) {
       int n = nums.length;
       int sum1 = 0;
      for(int num : nums){
         sum1 += num;
      }
      Set<Integer> set = new HashSet<>();
      for(int num : nums){
        set.add(num);
      }
      int sum2 = 0;
      for(int s : set){
        sum2 += s;
      }
      int diff1 = sum1 - sum2;
      int diff2 = n - set.size();
      int res = diff1/diff2;
      return res;
    }
}
