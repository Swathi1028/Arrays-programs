import java.io.*;

class GFG {
    public static void main (String[] args) {
        int[] arr = {10, 20, 30, 40}; 
        double avg = average(arr);   
        System.out.println("Average: " + avg);
    }
    public static double average(int[] arr) {
        int sum = 0; 
        for (int i = 0; i < arr.length; i++) {
            sum += arr[i];
        }
        double avg = (double) sum / arr.length;
        return avg; 
    }
}
