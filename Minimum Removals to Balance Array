class Solution {
    public int minRemoval(int[] nums, int k) {
        int n = nums.length;
        java.util.Arrays.sort(nums);
        int ans = n;
        int j =0;
        for(int i=0;i<n;i++){
            while(j < n && nums[j] <= (long)nums[i]*k){
                j++;
            }
            ans = Math.min(ans, n - (j - i));
        }
        return ans;
    }
}
