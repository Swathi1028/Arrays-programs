class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
       for(int[] row : matrix){
        int left = 0, right = row.length - 1;
        while(left <= right){
            int mid = left + (right - left) / 2;
            if(row[mid] == target)return true;
            else if(row[mid] < target){
                left = mid + 1;
            }
            else{
                right = mid - 1;
            }
        }
       }
        return false;
    }
}
