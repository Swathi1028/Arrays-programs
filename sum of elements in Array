import java.io.*;

class GFG {
    public static void main (String[] args) {
        int[] arr = {5, 6, 7, 8};
        sumOfArray(arr);
    }
    public static void sumOfArray(int[] arr) {
        int sum = 0; 
        for (int i = 0; i < arr.length; i++) {
            sum += arr[i]; 
        }
        System.out.print(sum);
    }
}
